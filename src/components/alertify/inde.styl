@import '~css/style/color.styl'
.alertify-notifier.ajs-right {
  top: 8%
  left 60%
}

.ajs-ok, .ajs-cancel {
  display: inline-block;
  padding: 6px 12px;
  margin-bottom: 0;
  font-size: 14px;
  font-weight: 400;
  line-height: 1.42857143;
  text-align: center;
  white-space: nowrap;
  vertical-align: middle;
  -ms-touch-action: manipulation;
  touch-action: manipulation;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  background-image: none;
  border: 1px solid transparent;
  border-radius: 4px;
}

.ajs-ok {
  color: $backgroutfff;
  background-color: $backgrout5cb85c;
  border-color: $backgrout5cb85c;
}

.ajs-cancel {
  color: $backgroutfff;
  background-color: $backgroutd9534f;
  border-color: $backgroutd9534f;
}